name: Example Workflow
on: push

  workflow_dispatch:

jobs:
  jobs1:
    uses:singaraju9441/Ansible/blob/main/.github/workflows/demo.yml@main

  job2:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v2
      - name: Run build
        run: echo "Building the project..."
